				<key>ActionName</key>
				<string>Run AppleScript</string>
				<key>ActionNameComment</key>
				<string>Select BOOTCAMP partition in Startup Disk and restart</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>set bootSystem to "windows"tell application "System Settings"	activate	set current pane to pane id "com.apple.Startup-Disk-Settings.extension"		repeat until exists window "Startup Disk"		delay 0.5	end repeat		tell application "System Events" to tell window "Startup Disk"		set promptedForPassword to false				repeat until exists button "Click the lock to prevent further changes." -- wait for admin rights			if not (exists sheet 1) then -- if password prompt is not open				if promptedForPassword = true then return -- stop the script if password prompt was cancelled								set promptedForPassword to true								click button "Click the lock to make changes."			end if						delay 0.5		end repeat				delay 3				set focused of list 1 of scroll area 1 to true				set systems to every group of list 1 of scroll area 1				-- go to the first system		repeat with n from 1 to count of systems			key code 123 -- left arrow		end repeat				-- go right and look for the bootSystem		repeat with n from 1 to count of systems			if value of static text 1 contains bootSystem then				click button "Restart…"				click button "Restart" of sheet 1				return -- exit the script			end if						key code 124 -- right arrow		end repeat			end tellend tell</string>
